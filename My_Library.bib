	eprinttype = {arxiv},
	eprint = {0905.2794 [quant-ph]},
	keywords = {Quantum Physics},
}

@article{boissonneault_dispersive_2009-2,
	title = {Dispersive regime of circuit {QED}: photon-dependent qubit dephasing and relaxation rates},
