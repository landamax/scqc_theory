	eprint = {cond-mat/0608693},
	keywords = {Condensed Matter - Superconductivity, Quantum Physics, Condensed Matter - Mesoscale and Nanoscale Physics},
	file = {Full Text:C\:\\Users\\mlamda\\Zotero\\storage\\THQFDHK8\\Schuster et al. - 2007 - Resolving photon number states in a superconductin.pdf:application/pdf},
