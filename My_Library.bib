	urldate = {2023-03-12},
	date = {2009-01-23},
	eprinttype = {arxiv},
	eprint = {0810.1336 [cond-mat, physics:quant-ph]},
	keywords = {Condensed Matter - Superconductivity, Quantum Physics, Condensed Matter - Mesoscale and Nanoscale Physics},
	file = {arXiv Fulltext PDF:C\:\\Users\\mlamda\\Zotero\\storage\\7VIJKCYY\\Boissonneault et al. - 2009 - Dispersive regime of circuit QED photon-dependent.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\mlamda\\Zotero\\storage\\ESZGCDHP\\0810.html:text/html},
